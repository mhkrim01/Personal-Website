<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @*<link href="~/Content/Site.css" rel="stylesheet" type="text/css" />*@
    @*<link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />*@
    @*<link href="~/Content/particles.css" rel="stylesheet" type="text/css" />*@
    @System.Web.Optimization.Styles.Render("~/bundles/css")

    @System.Web.Optimization.Styles.Render("~/Content/css")
</head>


<body>

    @Html.Partial("Navbar")
    <div class="row">
        <div class="col-lg-6" @*style="background-color: floralwhite;*@>
            @RenderBody()
        </div>
    </div>

    @System.Web.Optimization.Scripts.Render("~/bundles/common-js")
    @System.Web.Optimization.Scripts.Render("~/bundles/bootstrap")
    @System.Web.Optimization.Scripts.Render("~/bundles/particles")
    @*<script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>*@
    <script src="~/Scripts/apps.js"></script>
</body>
</html>