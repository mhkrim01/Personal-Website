<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @*<link href="~/Content/Site.css" rel="stylesheet" type="text/css" />*@
    @*<link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />*@
    @*<link href="~/Content/particles.css" rel="stylesheet" type="text/css" />*@
    <link href="https://fonts.googleapis.com/css?family=Archivo+Narrow:400,400i,500,500i,600,600i,700,700i" rel="stylesheet" type="text/css">

    @System.Web.Optimization.Styles.Render("~/bundles/css")

    @System.Web.Optimization.Styles.Render("~/Content/css")
</head>


<body>
    <div id="container">
        <div id="header">
            @Html.Partial("Navbar")
        </div>
        <div id="body">
            @RenderBody()
        </div>
        <div id="footer">
            @Html.Partial("Footer")
        </div>
    </div>

    @System.Web.Optimization.Scripts.Render("~/bundles/common-js")
    @System.Web.Optimization.Scripts.Render("~/bundles/bootstrap")
    @System.Web.Optimization.Scripts.Render("~/bundles/particles")
    @*<script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>*@
    <script src="~/Scripts/app.js"></script>
</body>
</html>