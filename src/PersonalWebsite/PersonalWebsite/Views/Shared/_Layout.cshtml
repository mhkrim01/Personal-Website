<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="icon" href="~/Content/Pictures/favicon-16x16.png" />
    <!-- Google Analytics -->
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-120225268-1', 'auto');
        ga('send', 'pageview');
    </script>
    <!-- End Google Analytics -->

    @System.Web.Optimization.Styles.Render("~/bundles/css")

    @System.Web.Optimization.Styles.Render("~/Content/css")

</head>


<body>
    <div id="container">
        <div id="header">
            @Html.Partial("Navbar")
        </div>
        <div id="body">
            @RenderBody()
        </div>
        <div id="footer">
            @Html.Partial("Footer")
        </div>
    </div>

    <div id="particles-js"></div>@*particle background*@

    @System.Web.Optimization.Scripts.Render("~/bundles/common-js", "~/bundles/bootstrap", "~/bundles/particles", "~/bundles/app-js")
    @RenderSection("pagescripts")
</body>
</html>